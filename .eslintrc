{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-useless-constructor": 2,
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "no-unused-vars": [0, { "args": "all" }],
    "max-len": [1, 110, 4],
    "react/prop-types": 1,
    "react/jsx-no-bind": 0,
    "key-spacing": [0, { "align": "value" }],
    "no-multi-spaces": [2, { exceptions: { "ImportDeclaration": true, "VariableDeclarator": true } }],
    "no-param-reassign": [2, { "props": false }],
    "quote-props": [2, "consistent-as-needed"],
    "no-constant-condition": 0,
    "no-empty-function": 0,
    "import/no-unresolved": 0,
    "new-cap": 0,
    "no-debugger": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "import/no-dynamic-require": 0,
    "react/forbid-prop-types": 0,
    "react/display-name": [0, { "ignoreTranspilerName": true }],
    "react/jsx-curly-spacing": [2, "always"],
    "import/imports-first": 0,
    "consistent-return": 0,
    "arrow-body-style": 0,
    "no-underscore-dangle": 0, #1
    "import/prefer-default-export": 0,
    "react/no-find-dom-node": 0,
    "require-yield": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/label-has-for": 0, #1
    "jsx-a11y/img-has-alt": 0, #1
    "react/no-unused-prop-types": 0, #1
    "react/no-unescaped-entities": 0, #1
    "react/prefer-stateless-function": 0,
    "react/no-string-refs": 0,
    "global-require": 0,
    "no-bitwise": 0,
    "class-methods-use-this": 0,
    "react/no-danger": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "classes": true,
      "generators": true,
      "experimentalObjectRestSpread": true
    },
  },
}